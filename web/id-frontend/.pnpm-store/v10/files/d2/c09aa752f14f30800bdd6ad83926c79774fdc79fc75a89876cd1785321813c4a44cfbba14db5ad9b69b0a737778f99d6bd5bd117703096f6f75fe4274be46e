> {\n    // FNV-1a offset basis\n    let hash = 0x811c9dc5;\n    const len = seed.length;\n\n    hash ^= len;\n    hash = Math.imul(hash, 0x01000193);\n\n    for (let i = 0; i < len; i++) {\n        hash ^= seed.charCodeAt(i);\n\n        hash = Math.imul(hash, 0x01000193);\n    }\n\n    hash ^= hash >>> 16;\n    hash = Math.imul(hash, 0x85ebca6b);\n    hash ^= hash >>> 13;\n    hash = Math.imul(hash, 0xc2b2ae35);\n    hash ^= hash >>> 16;\n\n    hash = Math.imul(hash, 0x27d4eb2d);\n    hash ^= hash >>> 15;\n\n    return hash | 0;\n};\n\nexport const normalizeHash = (hash: number, min: number, max: number) => {\n    const absHash = Math.abs(hash);\n\n    return Math.floor((absHash % (max - min + 1)) + min);\n};\n\nexport const extractHashPart = (hash: number, offset: number): number => {\n    // mix hash with offset to get independent values\n    let part = hash ^ (offset * 0x9e3779b9);\n\n    // mix to improve distribution\n    part ^= part >>> 16;\n    part = Math.imul(part, 0x85ebca6b);\n    part ^= part >>> 13;\n    part = Math.imul(part, 0xc2b2ae35);\n    part ^= part >>> 16;\n\n    return part >>> 0;\n};\n"]}