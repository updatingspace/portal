var colorKeywords = lib.reduce((obj, key, index) => {
  obj[key] = colors_named_hex_lib[index];
  return obj;
}, {});
var baseNamed = ['aqua', 'black', 'blue', 'fuchsia', 'gray', 'green', 'lime', 'maroon', 'navy', 'olive', 'purple', 'red', 'silver', 'teal', 'white', 'yellow'];
var colorKeywordsBase = baseNamed.reduce((obj, key) => {
  obj[key] = colorKeywords[key];
  return obj;
}, {});
function colorNameToHex(name) {
  return colorKeywords[name];
}
