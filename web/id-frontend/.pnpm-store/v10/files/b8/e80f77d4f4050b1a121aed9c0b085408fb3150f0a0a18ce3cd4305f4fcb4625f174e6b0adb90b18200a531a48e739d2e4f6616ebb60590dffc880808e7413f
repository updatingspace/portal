export interface ColorPickerProps {
    size?: 's' | 'm' | 'l' | 'xl';
    value?: string;
    defaultValue?: string;
    onUpdate?: (value: string) => void;
    open?: boolean;
    defaultOpen?: boolean;
    onOpenChange?: (open: boolean) => void;
    withAlpha?: boolean;
    compact?: boolean;
}
export declare const ColorPicker: ({ size, value, defaultValue, onUpdate, open, onOpenChange, defaultOpen, withAlpha, compact, }: ColorPickerProps) => import("react/jsx-runtime").JSX.Element;
