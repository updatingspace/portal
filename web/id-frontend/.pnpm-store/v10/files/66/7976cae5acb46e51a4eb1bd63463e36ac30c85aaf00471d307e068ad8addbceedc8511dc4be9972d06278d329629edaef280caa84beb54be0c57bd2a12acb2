import type { QAProps } from "../types.js";
import "./Loader.css";
export type LoaderSize = 's' | 'm' | 'l';
export interface LoaderProps extends QAProps {
    className?: string;
    size?: LoaderSize;
}
export declare function Loader({ size, className, qa }: LoaderProps): import("react/jsx-runtime").JSX.Element;
