Group = true;\n        } else {\n            if (item.hidden) {\n                continue;\n            }\n\n            if (addedGroup) {\n                updatedItems.push(separator);\n            }\n\n            const updatedItem = {\n                ...item,\n                path: [...path, index++],\n            } as unknown as ListItem;\n\n            if (item.items) {\n                updatedItem.items = toItemList<Item, ListItem>(\n                    item.items,\n                    separator,\n                    updatedItem.path,\n                );\n            }\n\n            updatedItems.push(updatedItem);\n            addedGroup = false;\n        }\n    }\n\n    return updatedItems;\n}\n"]}