import { jsx as _jsx } from "react/jsx-runtime";
import * as React from 'react';
import { DEFAULT_AVATAR_SIZE } from "../Avatar/index.js";
import { block } from "../utils/cn.js";
import { AvatarStackMore } from "./AvatarStackMore.js";
const b = block('avatar-stack');
export const AvatarStackMoreButton = React.forwardRef(({ className, badgeClassName, size = DEFAULT_AVATAR_SIZE, onClick, count, 'aria-label': ariaLabel, borderColor, }, ref) => {
    return (_jsx("button", { ref: ref, type: "button", className: b('more-button', { size }, className), onClick: onClick, children: _jsx(AvatarStackMore, { className: badgeClassName, size: size, count: count, "aria-label": ariaLabel, borderColor: borderColor }) }));
});
AvatarStackMoreButton.displayName = 'AvatarStack.MoreButton';
//# sourceMappingURL=AvatarStackMoreButton.js.map
