import type { QAProps } from "../types.js";
import "./ArrowToggle.css";
export interface ArrowToggleProps extends QAProps {
    size?: number;
    direction?: 'top' | 'left' | 'bottom' | 'right';
    className?: string;
}
export declare function ArrowToggle({ size, direction, className, qa }: ArrowToggleProps): import("react/jsx-runtime").JSX.Element;
