import { jsx as _jsx } from "react/jsx-runtime";
import * as React from 'react';
import { b } from "../../constants.js";
export const ColorPointer = React.forwardRef(({ left, top, transform }, ref) => (_jsx("button", { ref: ref, className: b('color-pointer'), style: {
        left,
        top,
        transform,
    }, tabIndex: -1 })));
ColorPointer.displayName = 'ColorPointer';
