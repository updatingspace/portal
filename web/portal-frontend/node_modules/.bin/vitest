#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mihhailmatvejev/WebstormProjects/aef-vote/web/portal-frontend/node_modules/.pnpm/vitest@4.0.17_@types+node@24.10.9_jsdom@25.0.1/node_modules/vitest/node_modules:/Users/mihhailmatvejev/WebstormProjects/aef-vote/web/portal-frontend/node_modules/.pnpm/vitest@4.0.17_@types+node@24.10.9_jsdom@25.0.1/node_modules:/Users/mihhailmatvejev/WebstormProjects/aef-vote/web/portal-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mihhailmatvejev/WebstormProjects/aef-vote/web/portal-frontend/node_modules/.pnpm/vitest@4.0.17_@types+node@24.10.9_jsdom@25.0.1/node_modules/vitest/node_modules:/Users/mihhailmatvejev/WebstormProjects/aef-vote/web/portal-frontend/node_modules/.pnpm/vitest@4.0.17_@types+node@24.10.9_jsdom@25.0.1/node_modules:/Users/mihhailmatvejev/WebstormProjects/aef-vote/web/portal-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
